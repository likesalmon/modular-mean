{
    "camelcase": false,
    "curly": false,
    "eqeqeq": true,
    "expr" : true,
    "forin": true,
    "globals": {
        "angular": true
    },
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "mocha": true,
    "newcap": false,
    "noarg": true,
    "node": true,
    "nonbsp": true,
    "predef": [
        "browser",
        "by",
        "element",
        "it",
        "describe",
        "beforeEach",
        "afterEach"
    ],
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": "vars"
}
